# Calcola la differenza tra due bit A e B.
.model SUB1

# Accetta tre input: due bit A e B ed un
# bit di prestito BIN.
# Risponde un bit di risultato RES, ed un
# bit di prestito BOUT.
.inputs  A B BIN
.outputs RES BOUT

# Nodo che calcola la differenza.
.names A B BIN RES
    001 1
    010 1
    100 1
    111 1

# Nodo che calcola il prestito.
.names A B BIN BOUT
    001 1
    010 1
    011 1
    111 1

.end
